<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <TextView
        android:id="@+id/txt_dd"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Di động"
        android:textSize="@dimen/text_18"
        android:padding="@dimen/padding_16"
        android:layout_alignParentTop="true"
        />
    <TextView
        android:id="@+id/txt_cq"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Cơ quan"
        android:textSize="@dimen/text_18"
        android:padding="@dimen/padding_16"
        android:layout_below="@+id/txt_dd" />
    <TextView
        android:id="@+id/txt_nr"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Nhà riêng"
        android:textSize="@dimen/text_18"
        android:padding="@dimen/padding_16"
        android:layout_below="@+id/txt_cq" />
    <TextView
        android:id="@+id/txt_sc"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Số chính"
        android:textSize="@dimen/text_18"
        android:padding="@dimen/padding_16"
        android:layout_below="@+id/txt_nr"
        />
    <TextView
        android:id="@+id/txt_sfc1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Số fax cơ quan"
        android:textSize="@dimen/text_18"
        android:padding="@dimen/padding_16"
        android:layout_below="@+id/txt_sc"
        />
    <TextView
        android:id="@+id/txt_sfnr"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="số fax nhà riêng"
        android:textSize="@dimen/text_18"
        android:padding="@dimen/padding_16"
        android:layout_below="@+id/txt_sfc1"
        />
    <TextView
        android:id="@+id/txt_smnt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Số máy nhắn tin"
        android:textSize="@dimen/text_18"
        android:padding="@dimen/padding_16"
        android:layout_below="@+id/txt_sfnr"
        />
    <TextView
        android:id="@+id/txt_k"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Khác"
        android:textSize="@dimen/text_18"
        android:padding="@dimen/padding_16"
        android:layout_below="@+id/txt_smnt"
        />
    <TextView
        android:id="@+id/txt_tc"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Tuỳ chỉnh"
        android:textSize="@dimen/text_18"
        android:padding="@dimen/padding_16"
        android:layout_below="@+id/txt_k"
        />

</RelativeLayout>